{
	"name": "outfile_csv_dot_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FACTORY": {
				"type": "string"
			},
			"TABLENAME": {
				"type": "string"
			},
			"SYSTEM": {
				"type": "string"
			},
			"DATE": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@{dataset().TABLENAME}_@{dataset().DATE}.csv",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@{dataset().FACTORY}/@{dataset().TABLENAME}",
					"type": "Expression"
				},
				"container": {
					"value": "datalakedata/@{dataset().SYSTEM}",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": false,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}