			},
			"TABLENAME": {
				"type": "string",
				"defaultValue": "SHOP_ORD_TAB"
			},
			"FACTORY": {
				"type": "string",
				"defaultValue": "CK"
			},
			"SYSTEM": {
				"type": "string",
				"defaultValue": "ifs"
			},
			"MERGE": {
				"type": "string",
				"defaultValue": "DELETE     FROM [CK].[SHOP_ORD_TAB]     WHERE EXISTS(SELECT TOP 1 1                  FROM  [CKSTG].[SHOP_ORD_TAB]                  WHERE [CKSTG].[SHOP_ORD_TAB].[ORDER_NO] = [CK].[SHOP_ORD_TAB].[ORDER_NO]                  AND   [CKSTG].[SHOP_ORD_TAB].[RELEASE_NO] = [CK].[SHOP_ORD_TAB].[RELEASE_NO]                  AND   [CKSTG].[SHOP_ORD_TAB].[SEQUENCE_NO] = [CK].[SHOP_ORD_TAB].[SEQUENCE_NO]                  )"
			},
			"STGSQL": {
				"type": "string",
