			"MERGE": {
				"type": "string",
				"defaultValue": "DELETE     FROM [CK].[PURCHASE_REQUISITION_TAB]     WHERE EXISTS(SELECT TOP 1 1                  FROM [CKSTG].[PURCHASE_REQUISITION_TAB]                  WHERE [CKSTG].[PURCHASE_REQUISITION_TAB].[REQUISITION_NO] = [CK].[PURCHASE_REQUISITION_TAB].[REQUISITION_NO])"
			},
			"STGSQL": {
				"type": "string",
				"defaultValue": "select REPLACE(requisition_no, CHR(13) || CHR (10), '')      requisition_no,        REPLACE(contract, CHR(13) || CHR (10), '')            contract,        REPLACE(order_code, CHR(13) || CHR (10), '')          order_code,        REPLACE(requisitioner_code, CHR(13) || CHR (10), '')  requisitioner_code,        REPLACE(mark_for, CHR(13) || CHR (10), '')            mark_for,        TO_CHAR(requisition_date, 'yyyy-mm-dd hh:mi:ss')           requisition_date,        REPLACE(authorize_id, CHR(13) || CHR (10), '')           authorize_id,        pre_accounting_id,        note_id,        REPLACE(destination_id, CHR(13) || CHR (10), '')        destination_id ,        REPLACE(internal_destination, CHR(13) || CHR (10), '')  internal_destination ,        REPLACE(rowstate, CHR(13) || CHR (10), '')              rowstate ,        TO_CHAR(rowversion, 'yyyy-mm-dd hh:mi:ss')              rowversion from IFSAPP.PURCHASE_REQUISITION_TAB where TO_CHAR(rowversion, 'yyyy-mm-dd hh:mi:ss') >"
			}
