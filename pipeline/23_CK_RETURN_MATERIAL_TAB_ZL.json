		"parameters": {
			"GetMAXDateSQL": {
				"type": "string",
				"defaultValue": "  SELECT CONVERT(NVARCHAR,ISNULL(max(CAST(ROWVERSION AS DATETIME)) ,'1900-01-01'),20) Batch            FROM [CK].[RETURN_MATERIAL_TAB]"
			},
			"TABLENAME": {
				"type": "string",
				"defaultValue": "RETURN_MATERIAL_TAB"
			},
			"FACTORY": {
				"type": "string",
				"defaultValue": "CK"
			},
			"SYSTEM": {
				"type": "string",
				"defaultValue": "ifs"
			},
			"MERGE": {
				"type": "string",
				"defaultValue": " DELETE     FROM [CK].[RETURN_MATERIAL_TAB]     WHERE EXISTS(SELECT TOP 1 1                  FROM [CKSTG].[RETURN_MATERIAL_TAB]                  WHERE [CKSTG].[RETURN_MATERIAL_TAB].[RMA_NO] = [CK].[RETURN_MATERIAL_TAB].[RMA_NO])"
			},
			"STGSQL": {
				"type": "string",
				"defaultValue": "select rma_no,        TO_CHAR(date_requested, 'yyyy-mm-dd hh:mi:ss')           date_requested,        REPLACE(currency_code, CHR(13) || CHR (10), '')            currency_code,        REPLACE(customer_no, CHR(13) || CHR (10), '')              customer_no,        REPLACE(contract, CHR(13) || CHR (10), '')                 contract,        REPLACE(return_approver_id, CHR(13) || CHR (10), '')       return_approver_id,        REPLACE(cust_ref, CHR(13) || CHR (10), '')                 cust_ref,        REPLACE(rma_report_printed, CHR(13) || CHR (10), '')       rma_report_printed,        REPLACE(language_code, CHR(13) || CHR (10), '')            language_code,        note_id ,        REPLACE(note_text, CHR(13) || CHR (10), '')                       note_text,        REPLACE(customer_no_addr_no, CHR(13) || CHR (10), '')             customer_no_addr_no,        REPLACE(customer_no_credit, CHR(13) || CHR (10), '')              customer_no_credit,        REPLACE(customer_no_credit_addr_no, CHR(13) || CHR (10), '')      customer_no_credit_addr_no,        REPLACE(vat, CHR(13) || CHR (10), '')                             vat,        REPLACE(ship_addr_no, CHR(13) || CHR (10), '')                    ship_addr_no,        REPLACE(rowstate, CHR(13) || CHR (10), '')                        rowstate,        TO_CHAR(rowversion, 'yyyy-mm-dd hh:mi:ss')           rowversion,        REPLACE(price_with_tax, CHR(13) || CHR (10), '')            price_with_tax,        n_receipt_year ,        n_receipt_month ,        REPLACE(n_receipt_period, CHR(13) || CHR (10), '')        n_receipt_period,        REPLACE(n_part_type, CHR(13) || CHR (10), '')             n_part_type,        TO_CHAR(n_from_date, 'yyyy-mm-dd hh:mi:ss')        n_from_date,        TO_CHAR(n_to_date, 'yyyy-mm-dd hh:mi:ss')          n_to_date,        REPLACE(n_receipt_control, CHR(13) || CHR (10), '')       n_receipt_control         from IFSAPP.RETURN_MATERIAL_TAB where TO_CHAR(rowversion, 'yyyy-mm-dd hh:mi:ss') >"
			}
		},
