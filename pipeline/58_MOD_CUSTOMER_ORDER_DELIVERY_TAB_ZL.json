			},
			"STGSQL": {
				"type": "string",
				"defaultValue": "select deliv_no,        REPLACE(order_no, CHR(13) || CHR (10), '')                 order_no,        REPLACE(line_no, CHR(13) || CHR (10), '')                  line_no,        REPLACE(rel_no, CHR(13) || CHR (10), '')                   rel_no,        line_item_no,        REPLACE(component_invoice_flag, CHR(13) || CHR (10), '')   component_invoice_flag,        load_id,          REPLACE(delnote_no, CHR(13) || CHR (10), '')               delnote_no,        TO_CHAR(date_delivered, 'yyyy-mm-dd hh:mi:ss')             date_delivered,        qty_shipped,        qty_to_invoice,        qty_invoiced,        TO_CHAR(rowversion, 'yyyy-mm-dd hh:mi:ss')                rowversion,        REPLACE(ivp_invoice_no, CHR(13) || CHR (10), '')          ivp_invoice_no  from IFSAPP.CUSTOMER_ORDER_DELIVERY_TAB where TO_CHAR(rowversion, 'yyyy-mm-dd hh:mi:ss') >"
			}
